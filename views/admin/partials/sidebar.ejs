<!-- Page Sidebar Start-->
<div class="sidebar-wrapper" data-sidebar-layout="stroke-svg">
  <div>
    <div class="logo-wrapper">
      <a href="/admin/dashboard">
        <% if (logoSettings && logoSettings.dark_logo && logoSettings.white_logo) { %>
          <img class="img-fluid for-light" src="<%= logoSettings.dark_logo %>" alt="Logo" />
          <img class="img-fluid for-dark" src="<%= logoSettings.white_logo %>" alt="Logo" />
        <% } else if (logoSettings && logoSettings.dark_logo) { %>
          <img class="img-fluid for-light" src="<%= logoSettings.dark_logo %>" alt="Logo" />
        <% } else if (logoSettings && logoSettings.white_logo) { %>
          <img class="img-fluid for-dark" src="<%= logoSettings.white_logo %>" alt="Logo" />
        <% } else { %>
          <img class="img-fluid for-light" src="<%= assetPath %>/images/logo/landing-logo.png" alt="Logo" />
          <img class="img-fluid for-dark" src="<%= assetPath %>/images/logo/logo-white.png" alt="Logo" />
        <% } %>
      </a>
      <div class="back-btn">
        <i class="fa-solid fa-angle-left"></i>
      </div>
      <div class="toggle-sidebar">
        <i class="status_toggle middle sidebar-toggle" data-feather="grid"> </i>
      </div>
    </div>
    <div class="logo-icon-wrapper">
      <a href="/admin/dashboard">
        <% if (logoSettings && logoSettings.app_logo ) { %>
          <img class="img-fluid for-dark" src="<%= logoSettings.app_logo  %>" alt="looginpage" />
