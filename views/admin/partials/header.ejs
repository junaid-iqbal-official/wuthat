<!-- Page Header Start-->
<div class="page-header">
  <div class="header-wrapper row m-0">
    <form class="form-inline search-full col" method="get">
      <div class="form-group w-100">
        <div class="Typeahead Typeahead--twitterUsers">
          <div class="u-posRelative">
            <input class="demo-input Typeahead-input form-control-plaintext w-100" type="text"
              placeholder="Search Anything Here..." name="q" title="" autofocus />
            <div class="spinner-border Typeahead-spinner" role="status">
              <span class="sr-only">Loading...</span>
            </div>
            <i class="close-search" data-feather="x"></i>
          </div>
          <div class="Typeahead-menu"></div>
        </div>
      </div>
    </form>
    <div class="header-logo-wrapper col-auto p-0">
      <div class="logo-wrapper">
        <a href="/admin/dashboard">
          <% if (logoSettings && logoSettings.dark_logo && logoSettings.white_logo) { %>
            <img class="img-fluid for-light" src="<%= logoSettings.dark_logo %>" alt="Logo" />
            <img class="img-fluid for-dark" src="<%= logoSettings.white_logo %>" alt="Logo" />
          <% } else if (logoSettings && logoSettings.dark_logo) { %>
            <img class="img-fluid for-light" src="<%= logoSettings.dark_logo %>" alt="Logo" />
          <% } else if (logoSettings && logoSettings.white_logo) { %>
            <img class="img-fluid for-dark" src="<%= logoSettings.white_logo %>" alt="Logo" />
          <% } else { %>
            <img class="img-fluid for-light" src="<%= assetPath %>/images/logo/landing-logo.png" alt="Logo" />
            <img class="img-fluid for-dark" src="<%= assetPath %>/images/logo/logo-white.png" alt="Logo" />
          <% } %>
        </a>
      </div>
      <div class="toggle-sidebar">
        <i class="status_toggle middle sidebar-toggle" data-feather="align-center"></i>
      </div>
    </div>
    <div class="nav-right col-xxl-7 col-xl-6 col-md-7 col-8 pull-right right-header p-0 ms-auto">
      <ul class="nav-menus">
        <li class="fullscreen-body">
          <span>
            <svg id="maximize-screen">
              <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#full-screen"></use>
            </svg>
          </span>
        </li>
        <li>
          <div class="mode">
            <svg>
              <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#moon"></use>
            </svg>
          </div>
        </li>
        <li class="profile-nav onhover-dropdown pe-0 py-0">
          <div class="d-flex profile-media">
            <% if (user.avatar) { %>
              <img class="b-r-10" src="<%= user.avatar %>" alt="" />
            <% } else { %>
              <div class="avatar-circle">
                <span class="initial">
                  <%= user.name.charAt(0).toUpperCase() %>
                </span>
              </div>
            <% } %>
            <div class="flex-grow-1">
              <span>
                <%= user.name %>
              </span>
              <p class="mb-0">Admin <i class="middle fa-solid fa-angle-down"></i></p>
            </div>
          </div>
          <ul class="profile-dropdown onhover-show-div">
            <li>
              <a href="/admin/account">
                <i data-feather="user"></i>
                <span> Account </span>
              </a>
            </li>
            <li>
              <a href="javascript:void(0)" data-logout>
                <i data-feather="log-in"> </i>
                <span>Log out</span>
              </a>
            </li>
          </ul>
        </li>
      </ul>
    </div>
  </div>
</div>
<!-- Page Header Ends-->