<div class="page-body">
  <!-- breadcrumb for title -->
  <%- include('../global/components/breadcrumb.ejs',{subTitle:"Reported Accounts",icon:"stroke-report"}) %>
    <!-- Container-fluid starts-->
    <div class="container-fluid user-list-wrapper">
      <div class="row"><%- include('../auth/components/error-alert.ejs', { error:error }) %></div>
      <div class="row">
        <div class="col-12">
          <div class="card report">
            <!-- Add Report Link -->
            <div class="card-header card-no-border text-end">
              <div class="card-header-right-icon">
                <a class="btn btn-primary f-w-500" href="javascript:void(0)" data-bs-toggle="modal"
                  data-bs-target="#addFAQ">
                  <i class="fa-solid fa-plus pe-2"></i>
                </a>
                <!-- Add FAQ pop-up -->
              </div>
            </div>
            <div class="card-body pt-0 px-0">
              <div class="list-product user-list-table">
                <div class="table-responsive custom-scrollbar">
                  <div id="roles-permission_wrapper" class="dt-container dt-empty-footer">
                    <%- include('../global/components/entries-search.ejs',{ tableId:"reportTable"}) %>
                      <!-- FAq List table -->
                      <div class="dt-layout-row dt-layout-table">
                        <div class="dt-layout-cell">
                          <table class="table dataTable" id="reportTable">
                            <!-- table header section -->
                            <thead>
                              <tr role="row">
                                <th rowspan="1" colspan="1" class="reportTable-sortable" data-field="id">
                                  <span>
                                    <span class="c-o-light f-w-600">Sr.</span>
                                  </span>
                                </th>
                                <th rowspan="1" colspan="1" class="reportTable-sortable" data-field="reporter_id">
                                  <span class="dt-column-title" role="button">
                                    <span class="c-o-light f-w-600">Reporter</span>
                                  </span>
                                </th>
                                <th rowspan="1" colspan="1" class="reportTable-sortable" data-field="reported_user_id">
                                  <span class="dt-column-title" role="button">
                                    <span class="c-o-light f-w-600">Reported</span>
                                  </span>
                                </th>
                                <th rowspan="1" colspan="1" class="reportTable-sortable" data-field="report_type">
                                  <span class="dt-column-title" role="button">
                                    <span class="c-o-light f-w-600">Report Type</span>
                                    <svg class="shorting-arrow-up">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-up"></use>
                                    </svg>
                                    <svg class="shorting-arrow-down">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-down"></use>
                                    </svg>
                                  </span>
                                </th>
                                <th rowspan="1" colspan="1" class="reportTable-sortable" data-field="description">
                                  <span class="dt-column-title" role="button">
                                    <span class="c-o-light f-w-600">Description</span>
                                    <svg class="shorting-arrow-up">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-up"></use>
                                    </svg>
                                    <svg class="shorting-arrow-down">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-down"></use>
                                    </svg>
                                  </span>
                                </th>
                                <th data-dt-column="6" rowspan="1" colspan="1" class="reportTable-sortable"
                                  data-field="status">
                                  <span class="dt-column-title" role="button">
                                    <span class="c-o-light f-w-600">Status</span>
                                    <svg class="shorting-arrow-up">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-up"></use>
                                    </svg>
                                    <svg class="shorting-arrow-down">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-down"></use>
                                    </svg>
                                  </span>
                                </th>
                                <th data-dt-column="6" rowspan="1" colspan="1" class="reportTable-sortable"
                                  data-field="admin_notes">
                                  <span class="dt-column-title" role="button">
                                    <span class="c-o-light f-w-600">Admin Notes</span>
                                    <svg class="shorting-arrow-up">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-up"></use>
                                    </svg>
                                    <svg class="shorting-arrow-down">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-down"></use>
                                    </svg>
                                  </span>
                                </th>
                                <th data-dt-column="6" rowspan="1" colspan="1" class="reportTable-sortable"
                                  data-field="resolved_by">
                                  <span class="dt-column-title" role="button">
                                    <span class="c-o-light f-w-600">Resolved By</span>
                                    <svg class="shorting-arrow-up">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-up"></use>
                                    </svg>
                                    <svg class="shorting-arrow-down">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-down"></use>
                                    </svg>
                                  </span>
                                </th>
                                <th data-dt-column="6" rowspan="1" colspan="1" class="reportTable-sortable"
                                  data-field="resolved_at">
                                  <span class="dt-column-title" role="button">
                                    <span class="c-o-light f-w-600">Resolved At</span>
                                    <svg class="shorting-arrow-up">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-up"></use>
                                    </svg>
                                    <svg class="shorting-arrow-down">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-down"></use>
                                    </svg>
                                  </span>
                                </th>
                                <th data-dt-column="6" rowspan="1" colspan="1" class="reportTable-sortable"
                                  data-field="created_at">
                                  <span class="dt-column-title" role="button">
                                    <span class="c-o-light f-w-600">Created At</span>
                                    <svg class="shorting-arrow-up">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-up"></use>
                                    </svg>
                                    <svg class="shorting-arrow-down">
                                      <use href="<%= assetPath %>/admin/svg/icon-sprite.svg#shorting-arrow-down"></use>
                                    </svg>
                                  </span>
                                </th>
                                <th data-dt-column="6" rowspan="1" colspan="1">
                                  <span class="dt-column-title" role="button">
                                    <span class="c-o-light f-w-600">Action</span>
                                  </span>
                                </th>
                              </tr>
                            </thead>

                            <!-- table body filled dynamically -->
                            <tbody></tbody>
                          </table>
                        </div>
                      </div>

                      <%- include('../global/components/table-pagination.ejs',{ tableId:"reportTable"}) %>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Container-fluid Ends-->
</div>

<%- include('./components/edit-report') %>